{
  "scripts": {
    "start": "parcel src/index.html",
    "configure": "cp \"config.$CONFIG.json\" src/config.json",
    "build": "rimraf dist && parcel build src/index.html",
    "deploy": "netlify deploy --prod --dir=dist --functions=netlify-functions",
    "prestart": "yarn run configure",
    "prebuild": "yarn run configure",
    "predeploy": "yarn run build"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "netlify-cli": "^2.59.0",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "tailwindcss": "^1.8.8",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vuex": "^3.5.1"
  }
}
